# Configurações de Produção - Dashboard Uno
# Estas variáveis serão usadas quando as credenciais reais da Imobzi estiverem disponíveis

# API Imobzi (configurar quando disponível)
VITE_IMOBZI_API_URL=https://api.imobzi.com/v1
VITE_IMOBZI_API_KEY=sua_chave_api_aqui
VITE_IMOBZI_CLIENT_ID=seu_client_id_aqui
VITE_IMOBZI_CLIENT_SECRET=seu_client_secret_aqui

# Configurações do Dashboard
VITE_UPDATE_INTERVAL=900000
VITE_CACHE_DURATION=300000
VITE_ENABLE_AUTO_UPDATE=true
VITE_ENABLE_CACHE=true
VITE_DEBUG_MODE=false

# Configurações de Performance
VITE_MAX_RETRIES=3
VITE_TIMEOUT=30000

